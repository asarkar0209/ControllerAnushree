{
	"name": "smoker_test1",
	"target_environment": "DEV",
	"pipelines": {
		"data_import_prep": {
		"components": {
        "data_fetch": {
          "mount_path": "/data",
          "build_version": "7",
          "command": [],
          "args": [
						"data_pull",
						"-in",
						{
							"inputValue": "inpath"
						},
						"-out",
						{
							"inputValue": "outpath"
						},
						"-r",
						{
							"inputValue": "repo_name"
						},
						"-b",
						{
							"inputValue": "branch_name"
						},
						"-c",
						{
							"inputValue": "commit"
						},
						"--type",
						"files",
						"-env",
						"qa"
					]
        },
        "data_prep": {
          "build_version": 7,
          "mount_path": "/data",
          "command": [],
          "args": [
						{
							"inputValue": "xpresso_run_name"
						}
          ]
        },
        "ml_train": {
          "build_version":7,
          "mount_path": "/data",
          "command": [],
          "args": [
						{
							"inputValue": "xpresso_run_name"
						},
						{
							"inputValue": "batch_size"
						},
						{
							"inputValue": "train_epochs"
						}
          ]
        }
      }
    }
  }
}
