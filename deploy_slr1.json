{
	"name": "slr_asarkar",
	"target_environment": "DEV",
	"components": {
            "data_fetch":{
                    "replicas" : 2,
                    "build_version" : 2},

	    "data_prep":{
                    "replicas" : 2,
                    "build_version" : 2},

	   "data_train":{
                    "replicas" : 2,
					"build_version" : 1}
				},
	
	
	"pipelines": {
		"prep_slr": {
		"components": {
        		"data_fetch": {
				"mount_path": "/data",
				"build_version": 1,
				"command": [],
				"args": [
						"data_pull",
						"-in",
						{
							"inputValue": "inpath"
						},
						"-out",
						{
							"inputValue": "outpath"
						},
						"-r",
						{
							"inputValue": "repo_name"
						},
						"-b",
						{
							"inputValue": "branch_name"
						},
						"-c",
						{
							"inputValue": "commit"
						},
						"--type",
						"files",
						"-env",
						"DEV"
					]},
         	    
		      "data_prep": {
				  "build_version": 1,
				  "mount_path": "/data",
				  "command": [],
				  "args": [
									{
										"inputValue": "xpresso_run_name"
									}
				  ]
				},
		    "data_train": {
				  "build_version": 1,
				  "mount_path": "/data",
				  "command": [],
				  "args": [
									{
										"inputValue": "xpresso_run_name"
									}
				  ]
				}
      }
    }
  }
}
